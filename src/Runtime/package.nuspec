<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>NuGetContentGenerator</id>
    <title>NuGet Content Generator</title>
    <version>$version$</version>
    <authors>Rodrigo Dumont</authors>
    <owners>Rodrigo Dumont</owners>
    <projectUrl>http://github.com/rdumont/NuGetContentGenerator</projectUrl>
    <licenseUrl>https://raw.github.com/rdumont/NuGetContentGenerator/master/LICENSE.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An MSBuild task to insert NuGet replacement tokens in C# files.</description>
    <tags>nuget pp content</tags>
    <releaseNotes>
        Initial release
    </releaseNotes>
      <dependencies>
          <dependency id="NuGetPowerTools" version="0.26" />
      </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\NuGetContentGenerator.dll" target="tools" />
    <file src="tools\*.*" target="tools" />
    <file src="content\readme.txt" target="content" />
  </files>
</package>
